static void Bintegrohv(B *sf, B *xf)
{
B *s, *x; D y, sum; P n;
s = (B *)VALUE_BASE(sf); n = ARRAY_SIZE(sf); x = (B *)VALUE_BASE(xf) + 1;
y = *s; *(s++) = sum = 0.0;
while (--n) { sum += (y + *s) * (*(x++)); y = *s; *(s++) = sum; }
