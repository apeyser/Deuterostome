static void Bintegroh(B *sf)
{
B *s; D y, sum; P n;
s = (B *)VALUE_BASE(sf); n = ARRAY_SIZE(sf);
y = *s; *(s++) = sum = 0.0;
while (--n) { sum += y + *s; y = *s; *(s++) = sum; }
}

