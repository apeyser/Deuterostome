static void Bextr(B *sf, D *min, D *max)
{
B *s; D t; P n;
s = (B *)VALUE_BASE(sf); n = ARRAY_SIZE(sf);
while (n--)
 { t = *(s++);
   if (t<*min) { *min = t; } else { if (t>*max) *max = t; }
 }
}

