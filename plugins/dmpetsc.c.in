    RETURN_ERROR(DMPETSC_KSPSOLVE_NOINIT);
  DMPETSC_ERRCHECK(KSPGetIterationNumber(ksp, &its));

  TAG(o_1) = (NUM|LONG32TYPE);
  ATTR(o_1) = 0;
  LONG32_VAL(o_1) = abs(its);

  return OK;
}

