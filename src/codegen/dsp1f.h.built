D *c, *cl; W k; L64 *s; D *d; P n,r;
s = (L64 *)VALUE_BASE(sf); d = (D *)VALUE_BASE(df);
n = ARRAY_SIZE(sf)-3; r = 1<<l2r;
cl = clb + ((r - l2r -1)<<2); s++;
while (n){ 
   *(d++) = *s; c = cl; k = r-1;
   while (k)
    { *(d++) = c[0] * s[-1] + c[1] * s[0] + c[2] * s[1] + c[3] * s[2];
      c += 4; k--; }
   s++; n--; }
*d = *s;
}

